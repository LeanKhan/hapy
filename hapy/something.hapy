class Person {
    has name;
    has school;
    has age = 22;

    def when_created() {
        print(self.name, " Was created!")
    };

    def when_string() {
        return "<Class>";
    };

    def when_printed() {
        return "[Class]";
    };

    def say_hello() {
       return self.name * 2;
    };
};